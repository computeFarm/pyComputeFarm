---

# This Ansible tasks file describes how to setup a testWorker

- name: Install the testWorker target
  template:
    src: testWorker.target.j2
    dest: "{{ ansible_facts.user_dir }}/.config/systemd/user/testWorker.target"
    mode: u=rw,g=r,o=r

- name: install multiple testWorkers
  include_tasks: repeatBlock
  loop: "{{ testWorkerList }}"
